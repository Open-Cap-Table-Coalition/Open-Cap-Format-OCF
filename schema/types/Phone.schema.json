{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://opencaptablecoalition.com/schema/types/phone",
  "title": "Type - Phone",
  "description": "Type representation of a phone number",
  "type": "object",
  "properties": {
    "phone_type": {
      "description": "Type of phone number (e.g. mobile, home or business)",
      "$ref": "/schema/enums/phone_type"
    },
    "phone_number": {
      "description": "A valid phone number",
      "type": "string"
    }
  },
  "required": ["phone_type", "phone_number"]
}
